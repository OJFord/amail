{
  "bundle": {
    "active": true,
    "targets": [],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTools",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "build": {
    "beforeBuildCommand": "rm -r dist; yarn; yarn run vite:build:debug-${TAURI_ENV_DEBUG}",
    "beforeDevCommand": "yarn; yarn run vite:dev --port=42411",
    "devUrl": "http://localhost:42411",
    "frontendDist": "../dist"
  },
  "productName": "amail",
  "version": "0.0.0",
  "identifier": "com.ojford.amail.eml-client",
  "plugins": {},
  "app": {
    "security": {
      "csp": {
        "default-src": "blob: data: filesystem: tauri: 'self'",
        "connect-src": "ipc://localhost data:",
        "style-src": "'unsafe-inline' tauri:"
      }
    },
    "windows": [
      {
        "title": "EML Viewer",
        "decorations": false,
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}
